{
  "name": "alabobai-unified",
  "version": "1.0.0",
  "description": "Alabobai Unified AI Platform - Your AI Operating System",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t alabobai-unified .",
    "docker:run": "docker run -p 8888:8888 --env-file .env alabobai-unified",
    "docker:compose": "docker-compose up -d",
    "sandbox:build": "docker build -t alabobai/sandbox:latest -f docker/sandbox/Dockerfile docker/sandbox",
    "db:backup": "./scripts/backup-db.sh",
    "db:restore": "./scripts/restore-db.sh"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "openai": "^4.70.0",
    "@deepgram/sdk": "^3.0.0",
    "dockerode": "^4.0.0",
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "cors": "^2.8.5",
    "compression": "^1.8.1",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "zod": "^3.22.0",
    "better-sqlite3": "^9.4.0",
    "puppeteer": "^22.0.0",
    "playwright": "^1.40.0",
    "glob": "^10.3.0",
    "sharp": "^0.33.0",
    "langchain": "^0.3.0",
    "@langchain/anthropic": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.8.0",
    "@types/uuid": "^9.0.7",
    "@types/better-sqlite3": "^7.6.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "optionalDependencies": {
    "robotjs": "^0.6.0"
  },
  "engines": {
    "node": ">=18 <23"
  }
}
