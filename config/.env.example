# Alabobai Unified Platform Configuration

# LLM Providers
ANTHROPIC_API_KEY=your_anthropic_key
OPENAI_API_KEY=your_openai_key

# Voice AI
DEEPGRAM_API_KEY=your_deepgram_key
ELEVENLABS_API_KEY=your_elevenlabs_key

# Server
PORT=8888
NODE_ENV=development

# Database
DATABASE_PATH=./data/alabobai.db

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_32_byte_encryption_key

# Guardian System
REQUIRE_APPROVAL_FOR_PAYMENTS=true
REQUIRE_APPROVAL_FOR_EMAILS=true
REQUIRE_APPROVAL_FOR_FILE_DELETE=true
AUTO_APPROVE_RESEARCH=true
AUTO_APPROVE_ADVICE=true

# Agent Configuration
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT_MS=300000
DEFAULT_LLM=claude-sonnet-4-20250514

# Computer Control
ENABLE_SCREEN_CONTROL=true
SCREENSHOT_INTERVAL_MS=1000
ENABLE_MOUSE_CONTROL=true
ENABLE_KEYBOARD_CONTROL=true

# Builder Integration
BOLT_DIY_ENABLED=true
DEFAULT_DEPLOY_TARGET=vercel

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/alabobai.log
